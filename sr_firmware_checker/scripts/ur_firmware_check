#!/bin/bash
timeout 5 echo "polyscopeversion" | nc -w2 $1 $2 | 
while read -r cmd; do
  while read firmware; do
    if [[ ${firmware}* -eq $cmd ]]; then
      echo "UR version $cmd is supported by Shadow"; kill $$;;
    fi
    echo "UR version $cmd is NOT supported by Shadow. Use our software at your own risk"; kill $$
  done <../config/known_good_firmware.txt
done
echo "UR robot did not respond"



